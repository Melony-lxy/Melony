<view class="coursecenter"><view class="information"><block wx:if="{{people}}"><view class="information_left"><image class="avatar _img" src="{{$root.g0}}"></image><view class="information_people"><view class="information_name">{{''+people.nick+''}}</view><view class="information_class">合伙人</view></view></view></block><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/rules/rules']]]]]}}" class="tip" bindtap="__e"><image class="tipImg" src="{{$root.g1}}" mode="widthFix"></image><view>合伙人规则</view></view></view><view class="wallet"><view class="wallet_left"><view>{{''+(type?'待结算金额(元)':'累计订单(笔)')+''}}</view><view class="money">{{''+(type?people.settlementPrice/100:people.orderCount)+''}}</view></view></view><view class="order"><view class="title"><view class="left_line"></view><view class="TitleText">{{type?'待结算金额列表':'累计订单列表'}}</view></view><block wx:if="{{list}}"><view class="OrderList"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="ordername">{{'订单号: '+item.$orig.order_number+''}}</view><view class="OrderDetails"><view class="avatarimg"><block wx:if="{{item.$orig.user}}"><image src="{{item.g2}}" alt class="_img"></image></block><view><view class="classorder">{{'用户:  '+(item.$orig.user?item.$orig.user.nick:'无效用户')+''}}</view><view class="classorder">{{'订单金额: ￥'+item.$orig.price/100+''}}</view><view class="classorder">{{'分销比例: '+item.$orig.distribution_ratio+'%'}}</view><view class="classorder">{{'支付时间: '+item.$orig.payment_time+''}}</view></view></view><view class="OrderMoney" style="{{'color:'+(item.$orig.type!=0?'red':'red')+';'}}">{{''+item.$orig.distribution_price/100+'元'}}</view></view></view></block></view></block><block wx:if="{{showtoast}}"><view class="nomore">暂无更多记录</view></block></view></view>