<view class="coursecenter"><view class="information"><block wx:if="{{people}}"><view class="information_left"><image class="avatar _img" src="{{$root.g0}}"></image><view class="information_people"><view class="information_name">{{''+people.nick+''}}</view><view class="information_class">合伙人</view></view></view></block><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/rules/rules']]]]]}}" class="tip" bindtap="__e"><image class="tipImg" src="{{$root.g1}}" mode="widthFix"></image><view>合伙人规则</view></view></view><view class="wallet"><view class="wallet_left"><view>账户总余额(元)</view><block wx:if="{{people}}"><view class="money">{{''+(people.balance-people.amountWithdrawal)/100+''}}</view></block></view><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/cashWithdrawal/cashWithdrawal']]]]]}}" class="withdrawal" bindtap="__e">提现</view></view><view class="order"><block wx:if="{{people}}"><view class="calculation"><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/accumulatedIncome/accumulatedIncome']]]]]}}" class="CalculationBox" bindtap="__e"><view>累计收益</view><view>（元）</view><view class="num">{{people.distributionPrice/100}}</view></view><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/cumulativeOrders/cumulativeOrders?type=1']]]]]}}" class="CalculationBox" bindtap="__e"><view>待结算金额</view><view>（元）</view><view class="num">{{people.settlementPrice/100}}</view></view><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/cumulativeCustomers/cumulativeCustomers']]]]]}}" class="CalculationBox" bindtap="__e"><view>累计客户</view><view>（人）</view><view class="num">{{people.distributionCount}}</view></view><view data-event-opts="{{[['tap',[['navigateTo',['/packgeB/cumulativeOrders/cumulativeOrders']]]]]}}" class="CalculationBox" bindtap="__e"><view>累计订单</view><view>（笔）</view><view class="num">{{people.orderCount}}</view></view></view></block><view class="title"><view class="flex"><view class="left_line"></view><view class="TitleText">余额变动记录</view></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="flex" bindtap="__e"><image src="{{$root.g2}}" alt class="_img"></image><view class="dressingbyscreening">筛选</view></view></view><block wx:if="{{list}}"><view class="OrderList"><block wx:for="{{list.data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="OrderDetails"><view><view class="OrderTitle">{{''+item.change_reason+''}}</view><view class="OrderWhy">{{''+item.ctime+''}}</view></view><view class="OrderMoney" style="{{'color:'+(item.type==0?'red':'green')+';'}}">{{''+(item.type==0?'+':'-')+item.offset/100+'元'}}</view></view></block></view></block><block wx:if="{{showtoast}}"><view class="nomore">暂无更多记录</view></block></view><u-select vue-id="377e328e-1" list="{{lists}}" mode="single-column" value="{{show}}" data-event-opts="{{[['^confirm',[['confirm']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l"></u-select></view>