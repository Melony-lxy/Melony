.flexs {
	display: flex;
}
.flexsCenter {
	display: flex;
	justify-content: center;
	align-items: center;
}
.table-btn{
		text-decoration:none;
}
.SpaceBetween {
	display: flex;
	justify-content: space-between;
}
.flex-wrap {
	flex-wrap: wrap;
}
.flex {
	display: flex;
}
.bgWhite {
	background-color: white;
}
/*垂直居中*/
.align-center {
	display: flex;
	align-items: center;
}
/*垂直水平居中*/
.flex-center {
	display: flex;
	justify-content: center;
	align-items: center;
}
/*两端对齐*/
.space-between {
	display: flex;
	justify-content: space-between;
}
.space-around {
	display: flex;
	justify-content: space-around;
}
.flex-shrink {
	flex: 1;
}
.text-overflow {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
/* flex布局 */
.u-f,
.u-f-ac,
.u-f-ajc {
	display: flex;
}
.u-f-ac,
.u-f-ajc {
	align-items: center;
}
.u-f-ajc {
	justify-content: center;
}
.u-f-jsb {
	justify-content: space-between;
}
.u-f1 {
	flex: 1;
}
.u-f-column {
	display: flex;
	flex-direction: column;
	justify-content: space-around;
	align-items: center;
}
.columnCenter {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.boxShadow {
	box-shadow: 0 0 5px rgba(184, 184, 184, 0.7);
}
.textCenter {
	text-align: center;
}
.textBold {
	font-weight: bold;
}
.tetxOverflow {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.textIndent {
	text-indent: 2em;
}
.centers{
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
}
.greenSpot{
	width: 12rpx;
	height: 12rpx;
	border-radius: 50%;
	background-color: #FF6638;
}
.spotName{
	font-size: 34rpx;
	font-weight: bold;
	color: #262626;
	margin-left: 10rpx;
}
.lines{
	width: 128rpx;
	height: 6rpx;
	background: #FF6638;
	border-radius: 4px;
	margin-top: 6rpx;
}
.bgGrey{
	background: rgb(249,250,251);
}
.margin20 {
		margin-top: 20rpx;
}
/*每个页面公共css */
uni-toast {
  z-index: 99999;
}
@font-face {
  font-family: fontXZJ;
  src: url("https://swan.imlimit.com/bahnschrift.ttf");
}
.priceItem,
.xprice {
  font-family: fontXZJ;
}
 .uni-tabbar-border {
  height: 1rpx !important;
  background-color: rgba(0, 0, 0, 0.2) !important;
}
.u-swiper-indicator {
  padding: 0 !important;
}
.u-btn--success {
  color: #ffffff;
  white-space: nowrap;
  border-color: #042BE3 !important;
  background-color: #042BE3 !important;
}
.u-success-hover {
  background: #042BE3 !important;
  color: #fff;
}
.uni-checkbox-input {
  border-radius: 50% !important;
  width: 35rpx !important;
  height: 35rpx !important;
}
.uni-checkbox-input-checked {
  background-color: #042BE3 !important;
  color: #fff !important;
  border-color: #042BE3 !important;
}
.LandingPage .u-indicator-item-round {
  background-color: #042BE3 !important;
  opacity: .2;
}
.LandingPage .u-indicator-item-round-active {
  background-color: #042BE3 !important;
  opacity: 1;
}
.uni-checkbox-input:hover {
  border-color: #fff !important;
}
checkbox .wx-checkbox-input {
  border-radius: 50% !important;
  width: 35rpx !important;
  height: 35rpx !important;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background-color: #042BE3 !important;
  color: #fff !important;
  border-color: #042BE3 !important;
}
.vjs-custom-skin > .video-js .vjs-big-play-button {
  background-color: #fffc !important;
}

