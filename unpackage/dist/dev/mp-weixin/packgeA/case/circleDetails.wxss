scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.nav-tab-wrap {
  max-width: 100%;
  white-space: nowrap;
}
.nav-tab-wrap .nav-tab {
  position: relative;
}
.nav-tab-wrap .nav-tab .nav-tab-item {
  margin-right: 60rpx;
  line-height: 80rpx;
  display: inline-block;
  color: #000;
  font-size: 28rpx;
}
.nav-tab-wrap .nav-tab .nav-tab-item.active {
  color: #042BE3;
  font-weight: 700;
}
.nav-tab-wrap .nav-tab .underline {
  position: absolute;
  height: 5rpx;
  background-color: #042BE3;
  bottom: 0;
  transition: 0.3s;
}

