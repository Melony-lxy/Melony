 .example-body {
  width: 100%;
  position: initial !important;
  top: 0%;
  background-color: white;
  z-index: 999;
}
 .padding1 {
  position: relative;
}
 uni-swiper {
  height: 1000rpx;
}
.top {
  padding-top: 60rpx;
  padding-left: 28rpx;
  background-repeat: no-repeat;
  background-size: 750rpx 700rpx;
  background-position: left bottom;
  padding-bottom: 43rpx;
}
.title {
  display: flex;
  align-items: center;
  margin-bottom: 35rpx;
}
.title .title-img {
  width: 155rpx;
  height: 155rpx;
}
.title .title-img ._img {
  width: 155rpx;
  height: 155rpx;
  object-fit: cover;
  border-radius: 50%;
}
.title .title-name {
  margin-left: 28rpx;
}
.title .title-name image {
  width: 32rpx;
  height: 32rpx;
  z-index: 1;
}
.title .title-name > .first-name {
  font-size: 44rpx;
  font-family: Source Han Sans SC-Medium, Source Han Sans SC;
  font-weight: 500;
  color: #ffffff;
}
.title .title-name > .second-img {
  margin-top: 13rpx;
  display: flex;
  align-items: center;
}
.title .title-name > .second-img text {
  font-size: 24rpx;
  font-family: Source Han Sans CN-Normal, Source Han Sans CN;
  font-weight: 400;
  color: #ffffff;
  margin-left: 8rpx;
}
