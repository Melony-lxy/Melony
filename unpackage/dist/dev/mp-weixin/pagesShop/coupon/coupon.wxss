.empty-u {
  height: 1000rpx;
  background-color: white;
}
 .line {
  height: 4rpx;
  background: #042BE3;
  box-shadow: 0 1rpx 5rpx white;
  position: absolute;
  bottom: 6rpx;
  z-index: 1;
  opacity: 0.7;
  transition: 0.3s;
  border-radius: 4rpx;
}
 .nav-one {
  font-size: 28rpx;
  font-family: Source Han Sans CN-Medium, Source Han Sans CN;
  font-weight: 500;
  color: #2d2d2d;
}
 .success {
  color: #042BE3 !important;
  font-weight: bold !important;
}
 .choosewhich {
  font-size: 28rpx !important;
  font-family: Source Han Sans CN-Medium, Source Han Sans CN !important;
  font-weight: bold !important;
  color: #ff6638;
}
.coupon_img {
  width: 100%;
  height: 350rpx;
  background: #042BE3;
  display: flex;
  justify-content: space-between;
}
.coupon_img .coupon_content {
  width: 40%;
  height: 100%;
}
.coupon_img .coupon_content .coupon_my {
  width: 100%;
  line-height: 200rpx;
  margin-left: 64rpx;
  font-size: 45rpx;
  font-weight: 500;
  color: #ffffff;
}
.coupon_img .coupon_content .coupon_num {
  width: 100%;
  line-height: 80rpx;
  margin-left: 64rpx;
  font-weight: 500;
  font-size: 60rpx;
  color: #ffffff;
  display: flex;
}
.coupon_img .coupon_images {
  height: 100%;
}
.coupon_img .coupon_images ._img {
  width: 400rpx;
  height: 290rpx;
  margin-top: 30rpx;
  padding-right: 20rpx;
}
.tab_tit {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-around;
}
.tab_tit ._li {
  list-style: none;
  width: 100rpx;
  height: 75rpx;
  line-height: 90rpx;
  font-size: 32rpx;
  text-align: center;
}
.active {
  color: #ff6638;
  border-bottom: 2rpx #ff6638 solid;
  font-weight: bold;
}
.notUsed,
.used,
.expired {
  width: 100%;
  height: auto;
  background-color: #f5f5f5;
  overflow: hidden;
}
.notUsed_coupon {
  width: 90%;
  height: 200rpx;
  margin: 5%;
  background-size: 100% 100%;
  overflow: hidden;
  display: flex;
}
.notUsed_coupon .notUsed_left {
  width: 25%;
  height: 100%;
  overflow: hidden;
}
.notUsed_coupon .notUsed_left .notUsed_money {
  margin-top: 55rpx;
  text-align: center;
  font-size: 45rpx;
  color: #ffe9bf;
}
.notUsed_coupon .notUsed_left .notUsed_fullUse {
  margin-top: 5rpx;
  text-align: center;
  font-size: 20rpx;
  color: #ffe9bf;
}
.notUsed_coupon .notUsed_right {
  width: 75%;
  height: 100%;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}
.notUsed_coupon .notUsed_right .notUsed_content1 {
  font-size: 33rpx;
  font-weight: bold;
  color: #ff6638;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.notUsed_coupon .notUsed_right .notUsed_content2 {
  font-size: 19rpx;
  font-weight: 400;
  color: #262626;
  margin-top: 15rpx;
  margin-left: 20rpx;
}
.notUsed_coupon .notUsed_right .notUsed_content3 {
  font-size: 19rpx;
  font-weight: 400;
  color: #989898;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.notUsed_coupon .notUsed_toUse {
  width: 100rpx;
  height: 50rpx;
  border-radius: 42rpx 42rpx 42rpx 42rpx;
  opacity: 1;
  border: 2rpx solid #ff6638;
  text-align: center;
  line-height: 50rpx;
  font-size: 22rpx;
  margin-top: 70rpx;
  margin-right: 20rpx;
  font-weight: 500;
  color: #ff6638;
}
.used_coupon,
.expired_coupon {
  width: 90%;
  height: 200rpx;
  margin: 5%;
  background-size: 100% 100%;
  display: flex;
}
.used_coupon ._img,
.expired_coupon ._img {
  object-fit: contain;
}
.used_coupon .used_left,
.used_coupon .expired_left,
.expired_coupon .used_left,
.expired_coupon .expired_left {
  width: 25%;
  height: 100%;
  overflow: hidden;
}
.used_coupon .used_left .notUsed_money,
.used_coupon .expired_left .notUsed_money,
.expired_coupon .used_left .notUsed_money,
.expired_coupon .expired_left .notUsed_money {
  margin-top: 55rpx;
  text-align: center;
  font-size: 45rpx;
  color: #ffffff;
}
.used_coupon .used_left .notUsed_fullUse,
.used_coupon .expired_left .notUsed_fullUse,
.expired_coupon .used_left .notUsed_fullUse,
.expired_coupon .expired_left .notUsed_fullUse {
  margin-top: 5rpx;
  text-align: center;
  font-size: 18rpx;
  color: #ffffff;
}
.used_coupon .used_right,
.used_coupon .expired_right,
.expired_coupon .used_right,
.expired_coupon .expired_right {
  width: 75%;
  height: 100%;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}
.used_coupon .used_right .notUsed_content1,
.used_coupon .expired_right .notUsed_content1,
.expired_coupon .used_right .notUsed_content1,
.expired_coupon .expired_right .notUsed_content1 {
  font-size: 33rpx;
  font-weight: bold;
  color: #999999;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.used_coupon .used_right .notUsed_content2,
.used_coupon .expired_right .notUsed_content2,
.expired_coupon .used_right .notUsed_content2,
.expired_coupon .expired_right .notUsed_content2 {
  font-size: 14rpx;
  font-weight: 400;
  color: #999999;
  margin-top: 15rpx;
  margin-left: 20rpx;
}
.used_coupon .used_right .notUsed_content3,
.used_coupon .expired_right .notUsed_content3,
.expired_coupon .used_right .notUsed_content3,
.expired_coupon .expired_right .notUsed_content3 {
  font-size: 14rpx;
  font-weight: 400;
  color: #999999;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.used_coupon .notUsed_img,
.expired_coupon .notUsed_img {
  height: 120rpx;
  margin-top: 40rpx;
  margin-right: 10rpx;
}
.used_coupon .notUsed_img ._img, .used_coupon .notUsed_img image, .expired_coupon .notUsed_img ._img, .expired_coupon .notUsed_img image {
  width: 130rpx;
  height: 100%;
}

