<view><block wx:if="{{people&&peoples}}"><view class="balance"><view class="my-balance">我的余额</view><view class="balance-num">{{''+(people.balance-people.amountWithdrawal)/100+''}}</view></view></block><view class="title"><view class="left_line"></view><view class="TitleText">提现金额</view></view><input class="balance-input" name="input" placeholder="请输入提现金额" type="digit" disabled="{{disabled}}" data-event-opts="{{[['input',[['__set_model',['','select','$event',[]]]]]]}}" value="{{select}}" bindinput="__e"/><view class="num"><block wx:for="{{Balance}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickNum',[index]]]]]}}" class="{{['_div',(selectPayment==index)?'active':'']}}" bindtap="__e">{{item.num}}</view></block></view><block wx:if="{{peoples}}"><view data-event-opts="{{[['tap',[['navigateTo']]]]}}" class="tips" bindtap="__e"><image src="{{$root.g0}}" alt class="_img"></image><text class="{{[(peoples.user.openid!='')?'isbinding':'']}}">{{peoples.user.openid!=''?'已绑定'+(peoples.user.wx_nick==null?'':':'+peoples.user.wx_nick):'去绑定'}}</text></view></block><view data-event-opts="{{[['tap',[['pushmoney']]]]}}" class="submit-button" bindtap="__e">申请提现</view></view>